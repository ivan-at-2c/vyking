{
  "name": "vyking-interview",
  "version": "1.0.0",
  "description": "Vyking Interview",
  "main": "index.js",
  "repository": "https://github.com/2C-Solution/vyking-app.git",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migration:generate": "sequelize-cli migration:generate",
    "test": "jest -i --colors --verbose --detectOpenHandles --runInBand --forceExit",
    "test:docker": "docker-compose -f ./../../docker-compose.test.yml -p docker-test up --build --abort-on-container-exit --exit-code-from backend-test --force-recreate",
    "test:docker-inject": "docker exec -t backend yarn test",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest -i --watchAll",
    "lint": "yarn lint:source",
    "lint:fix": "eslint --ext ts,tsx './src/**/*' --fix --quiet",
    "lint:source": "eslint './src/**/*' --quiet --cache --cache-strategy='content' --cache-location='.eslintcache/'"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.5",
    "@types/node": "^22.5.0",
    "@types/sequelize": "^4.28.16",
    "@types/supertest": "^2.0.15",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "coveralls": "^3.1.1",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.14",
    "@types/jsonwebtoken": "^9.0.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "http-status": "^1.7.0",
    "joi": "^17.10.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.9.5",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "pino-socket": "^7.4.0",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "umzug": "^3.3.1"
  }
}
